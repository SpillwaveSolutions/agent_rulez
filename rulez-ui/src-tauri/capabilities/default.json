{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default permissions for RuleZ UI",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:default",
    "store:default",
    "fs:default",
    "fs:allow-read-text-file",
    "fs:allow-write-text-file",
    "dialog:default",
    "dialog:allow-save",
    "clipboard-manager:default",
    "clipboard-manager:allow-write-text"
  ]
}
